<webscript kind="org.alfresco.repository.content.stream">
   <shortname>ESign</shortname>
   <description>
   Returns a user esign image in the format specified by the thumbnailname, or the "esign" preset if omitted.
   </description>
   <url>/slingshot/profile/esign/esign</url>
   <url>/slingshot/profile/esign/esign/thumbnail/{thumbnailname}</url>
   <url>/slingshot/profile/esign/{store_type}/{store_id}/{id}/thumbnail/{thumbnailname}</url>
   <url>/slingshot/profile/esign/{store_type}/{store_id}/{id}</url>
   <url>/slingshot/profile/esign/{username}/thumbnail/{thumbnailname}</url>
   <url>/slingshot/profile/esign/{username}</url>
   <format default="">argument</format>
   <authentication>user</authentication>
   <transaction>required</transaction>
   <lifecycle>internal</lifecycle>
</webscript>